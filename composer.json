    "description": "Symfony bundle used to execute search on Elasticsearch system based on simple query language",
    "keywords": ["search", "data", "elasticsearch", "query", "language"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ala Eddine Khefifi",
            "email": "alakhefifi@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "Nzo\\ElasticQueryBundle\\": "" }
    },
    "require": {
        "php": "^7.1.3",
        "symfony/framework-bundle": "^4.0",
        "friendsofsymfony/elastica-bundle": "^5.0",
        "justinrainbow/json-schema": "^5.2",
        "symfony/http-kernel": "^4.0",
        "symfony/http-foundation": "^4.0",
        "doctrine/orm": "^2.6"
